{
  "name": "@lrnwebcomponents/extensible-toolbar",
  "wcfactory": {
    "className": "ExtensibleToolbar",
    "customElementClass": "LitElement",
    "elementName": "extensible-toolbar",
    "generator-wcfactory-version": "0.7.1",
    "useHAX": false,
    "useSass": false,
    "files": {
      "css": "src/extensible-toolbar.css",
      "scss": "src/extensible-toolbar.scss",
      "html": "src/extensible-toolbar.html",
      "js": "src/extensible-toolbar.js",
      "properties": "src/extensible-toolbar-properties.json",
      "hax": "src/extensible-toolbar-hax.json"
    }
  },
  "version": "2.4.1",
  "description": "a toolbar that can be customised with with mixins and json",
  "repository": {
    "type": "git",
    "url": "https://github.com/elmsln/lrnwebcomponents.git"
  },
  "main": "extensible-toolbar.js",
  "module": "extensible-toolbar.es6.js",
  "umd": "extensible-toolbar.umd.js",
  "amd": "extensible-toolbar.amd.js",
  "scripts": {
    "test": "../../node_modules/.bin/wct --configFile ../../wct.conf.json node_modules/@lrnwebcomponents/extensible-toolbar/test/",
    "start": "yarn run dev",
    "build": "../../node_modules/.bin/gulp && ../../node_modules/.bin/rollup -c && ../../node_modules/.bin/prettier --ignore-path ../../.prettierignore --write '**/*.{js,json}'",
    "dev": "concurrently --kill-others \"yarn run watch\" \"yarn run serve\"",
    "watch": "../../node_modules/.bin/gulp dev",
    "serve": "polymer serve --npm --module-resolution=node --open",
    "lighthouse": "../../node_modules/.bin/gulp lighthouse"
  },
  "author": {
    "name": "nikkimk"
  },
  "license": "Apache-2.0",
  "dependencies": {
    "lit-element": "2.2.1"
  },
  "devDependencies": {
    "@lrnwebcomponents/deduping-fix": "2.0.5",
    "@polymer/iron-component-page": "github:PolymerElements/iron-component-page",
    "@polymer/iron-demo-helpers": "3.1.0",
    "@webcomponents/webcomponentsjs": "2.4.1",
    "concurrently": "5.1.0",
    "gulp-babel": "8.0.0",
    "gulp-sourcemaps": "2.6.5",
    "lodash": "4.17.15",
    "polymer-build": "3.1.4",
    "polymer-cli": "1.9.11",
    "wct-browser-legacy": "1.0.2",
    "web-animations-js": "2.3.2"
  },
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "web-components",
    "html"
  ]
}
