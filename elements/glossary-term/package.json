{
  "name": "@lrnwebcomponents/glossary-term",
  "wcfactory": {
    "className": "GlossaryTerm",
    "customElementClass": "LitElement",
    "elementName": "glossary-term",
    "generator-wcfactory-version": "0.7.0",
    "useHAX": true,
    "useSass": false,
    "files": {
      "css": "src/glossary-term.css",
      "scss": "src/glossary-term.scss",
      "html": "src/glossary-term.html",
      "js": "src/glossary-term.js",
      "properties": "src/glossary-term-properties.json",
      "hax": "src/glossary-term-hax.json"
    },
    "sharedStyles": []
  },
  "version": "2.4.4",
  "description": "Glossary element that connects to the glossary service.",
  "repository": {
    "type": "git",
    "url": "https://github.com/elmsln/lrnwebcomponents.git"
  },
  "type": "module",
  "main": "glossary-term.js",
  "module": "glossary-term.es6.js",
  "umd": "glossary-term.umd.js",
  "scripts": {
    "test": "../../node_modules/.bin/wct --configFile ../../wct.conf.json node_modules/@lrnwebcomponents/glossary-term/test/",
    "start": "yarn run dev",
    "build": "../../node_modules/.bin/gulp && ../../node_modules/.bin/rollup -c && ../../node_modules/.bin/prettier --ignore-path ../../.prettierignore --write '**/*.{js,json}' && wca analyze \"**/*.js\" --format vscode --outFile vscode-html-custom-data.json",
    "dev": "concurrently --kill-others 'yarn run watch' 'yarn run serve'",
    "watch": "../../node_modules/.bin/gulp dev",
    "serve": "../../node_modules/.bin/es-dev-server -c ../../es-dev-server.config.js",
    "lighthouse": "../../node_modules/.bin/gulp lighthouse"
  },
  "author": {
    "name": "heyMP"
  },
  "license": "Apache-2.0",
  "dependencies": {
    "@lrnwebcomponents/lrn-vocab": "^2.4.4",
    "@polymer/polymer": "^3.3.1",
    "@ui5/webcomponents": "^0.13.0"
  },
  "devDependencies": {
    "@polymer/iron-component-page": "github:PolymerElements/iron-component-page",
    "@polymer/iron-demo-helpers": "3.1.0",
    "@webcomponents/webcomponentsjs": "^2.2.10",
    "concurrently": "5.1.0",
    "gulp-babel": "8.0.0",
    "gulp-sourcemaps": "2.6.5",
    "lodash": "4.17.15",
    "polymer-build": "3.1.4",
    "polymer-cli": "1.9.11",
    "wct-browser-legacy": "1.0.2"
  },
  "private": true,
  "keywords": [
    "web-components",
    "html"
  ]
}
