    "useHAX": false,
    "useSass": true,
    "files": {
      "css": "src/chartist-render.css",
      "scss": "src/chartist-render.scss",
      "html": "src/chartist-render.html",
      "js": "src/chartist-render.js",
      "properties": "src/chartist-render-properties.json",
      "hax": "src/chartist-render-hax.json"
    },
    "sharedStyles": []
  },
  "version": "2.4.1",
