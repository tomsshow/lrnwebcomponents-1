{
  "name": "@lrnwebcomponents/simple-wc",
  "wcfactory": {
    "className": "SimpleWc",
    "customElementClass": "HTMLElement",
    "elementName": "simple-wc",
    "generator-wcfactory-version": "0.7.4",
    "useHAX": false,
    "useSass": false,
    "files": {
      "css": "src/simple-wc.css",
      "scss": "src/simple-wc.scss",
      "html": "src/simple-wc.html",
      "js": "src/simple-wc.js",
      "properties": "src/simple-wc-properties.json",
      "hax": "src/simple-wc-hax.json"
    }
  },
  "version": "2.4.1",
  "description": "A simple web component wrapper to simplify consistent patterns of element creation",
  "repository": {
    "type": "git",
    "url": "https://github.com/elmsln/lrnwebcomponents.git"
  },
  "type": "module",
  "main": "simple-wc.js",
  "module": "simple-wc.es6.js",
  "umd": "simple-wc.umd.js",
  "scripts": {
    "test": "../../node_modules/.bin/wct --configFile ../../wct.conf.json node_modules/@lrnwebcomponents/simple-wc/test/",
    "start": "yarn run dev",
    "build": "../../node_modules/.bin/gulp && ../../node_modules/.bin/rollup -c && ../../node_modules/.bin/prettier --ignore-path ../../.prettierignore --write '**/*.{js,json}' && wca analyze \"**/*.js\" --outFile custom-elements.json",
    "dev": "../../node_modules/.bin/concurrently --kill-others \"yarn run watch\" \"yarn run serve\"",
    "watch": "../../node_modules/.bin/gulp dev",
    "serve": "../../node_modules/.bin/es-dev-server -c ../../es-dev-server.config.js",
    "lighthouse": "../../node_modules/.bin/gulp lighthouse"
  },
  "author": {
    "name": "btopro"
  },
  "license": "Apache-2.0",
  "dependencies": {
    "lit-element": "^2.2.1"
  },
  "devDependencies": {
    "@lrnwebcomponents/deduping-fix": "^2.4.0",
    "@webcomponents/webcomponentsjs": "2.4.1",
    "concurrently": "5.1.0",
    "gulp-babel": "8.0.0",
    "gulp-sourcemaps": "2.6.5",
    "lodash": "4.17.15",
    "polymer-build": "3.1.4",
    "polymer-cli": "1.9.11",
    "wct-browser-legacy": "1.0.2",
    "web-animations-js": "2.3.2"
  },
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "web-components",
    "html"
  ]
}
